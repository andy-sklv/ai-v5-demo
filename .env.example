# ключ OpenAI
OPENAI_API_KEY=sk-...

# модель по умолчанию (в UI можно менять)
NEXT_PUBLIC_DEFAULT_MODEL=gpt-4o-mini

# какой vision-эндпоинт дергать из мини-панели:
# /api/vision (gpt-4o) или /api/vision41 (gpt-4.1-vision)
NEXT_PUBLIC_VISION_API=/api/vision41

# показать панель Toto (опционально)
NEXT_PUBLIC_TOTO_ENABLED=true

# куда проксирует /api/toto (если не используете Toto — можно не задавать)
# пример: локальный сервис на хосте
TOTO_PROXY_URL=http://host.docker.internal:8000

# флаг трассировки (оставлен для совместимости; без зависимости)
TOTO_TRACE=false
